{
  // Protocol plugins configuration
  "protocolPlugins": {
    // Default plugin to communicate with the load balancer
    "kuzzle-plugin-socketio": {
      "activated": true,
      "config": {
        "port": 7512,
        "room": "kuzzle"
      }
    }
  },
  "serverTimeout": 10000,
  // Server mode to choose between "failover" and "round-robin"
  // Default: failover
  "serverMode": "failover",
  //
  "serverOptions": {
    "port": 7331
  }
}