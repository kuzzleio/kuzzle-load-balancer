{
  // Protocol plugins configuration
  "protocolPlugins": {
    // Default plugin to communicate with the proxy
    "kuzzle-plugin-socketio": {
      "npmVersion": "1.1.0"
    }
  },
  // Timeout when a request is aborted and an error is returned in ms
  // Default: 10000
  "backendTimeout": 10000,
  // Backend mode to choose between "failover" and "round-robin"
  // Default: failover
  "backendMode": "failover",
  "backendOptions": {
    // Broker port waiting for connections
    "port": 7331
  },
  "httpPort": 7511
}