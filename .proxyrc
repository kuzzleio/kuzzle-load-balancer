{
  // Protocol plugins configuration
  "protocolPlugins": {
    // Default plugin to communicate with the load balancer
    "kuzzle-plugin-socketio": {
      "npmVersion": "1.1.0"
    }
  },
  // Timeout when a request is aborted and an error is returned in ms
  // Default: 10000
  "serverTimeout": 10000,
  // Server mode to choose between "failover" and "round-robin"
  // Default: failover
  "serverMode": "failover",
  "serverOptions": {
    // Broker port waiting for connections
    "port": 7331
  },
  "httpPort": 7511
}