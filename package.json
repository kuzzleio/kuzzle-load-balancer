    "bluebird": "^3.4.1",
    "bouncy": "^3.2.2",
    "bufferutil": "^1.2.1",
    "cli-color": "^1.1.0",
    "kuzzle-common-objects": "1.0.2",
    "lodash": "^4.14.1",
    "proper-lockfile": "^1.1.2",
    "rc": "^1.1.6",
    "utf-8-validate": "^1.2.1",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "eslint": "3.2.2",
    "istanbul": "0.4.4",
